{
  "build-version": "0.0.1",
  "dependencies": {
    "yum": [
      "atlas-devel",
      "blas-devel",
      "lapack-devel",
      "findutils",
      "gcc",
      "gcc-c++"
    ],
    "commands": [
      "yum downgrade -y http://packages.us-west-1.amazonaws.com/2018.03/main/c31535f74c6e/x86_64/Packages/libquadmath-6.4.1-1.45.amzn1.x86_64.rpm"
    ],
